<webscript>
	<shortname>Log out</shortname>
	<description><![CDATA[
  Log out, delete log in ticket.
  <BR>After the user has logged out the ticket is no longer valid and subsequent attempts to use it will fail.
  ]]>
	</description>
	<url>/tera/login/ticket/{ticket}</url>
	<format default="json" />
	<authentication>user</authentication>
	<transaction>required</transaction>
	<lifecycle>public_api</lifecycle>
	<family>tera</family>
	<args>
		<arg>
			<shortname>ticket</shortname>
			<description>The login ticket to invalidate.</description>
		</arg>
	</args>
</webscript>